ul(id="video-queue", class="jub-list")
script.
  // Server tells us about video state changes
  socket.on('queue', function(queue) {
    console.log('queue', queue);
    $('#video-queue').empty();
    for (video of queue) {
      var item = $('<li>').text(video['user'] + ': ' + video['title']);
      $('#video-queue').prepend(item);
    }
  });

  // Document is loaded so we can populate our video queue
  $( document ).ready(function() {
    socket.emit('queue', 0);
  });

